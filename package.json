{
  "name": "voho-saas",
  "version": "1.0.0",
  "description": "Multi-tenant SaaS with Ultravox integration",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "lint": "cd frontend && npm run lint",
    "lint:fix": "cd frontend && npm run lint:fix",
    "deploy-check": "node deploy-check.js",
    "deploy-fix": "node deploy-fix.js",
    "docker-test": "cd backend && docker build -t voho-backend-test . && docker run --rm -p 10000:10000 --env-file .env.example voho-backend-test npm run docker"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}

